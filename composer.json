{
  "name": "rhymedigital/contao_slick",
  "description": "Adds the ability to configure a Slick carousel https://github.com/kenwheeler/slick/ within Contao CMS",
  "keywords": [
    "slick",
    "carousel",
    "contao"
  ],
  "type": "contao-module",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Rhyme Digital",
      "homepage": "http://rhyme.digital"
    }
  ],
  "require": {
    "php": ">=5.3.2",
    "contao/core": ">=3.3.0,<4.0.0-dev"
  },
  "autoload": {
    "psr-0": {
      "Rhyme\\": [
        "library/"
      ]
    }
  },
  "extra": {
    "contao": {
      "sources": {
        "": "system/modules/slick"
      }
    }
  },
  "replace": {
    "hb-agency/contao_slick":"self.version"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist"
  }
}